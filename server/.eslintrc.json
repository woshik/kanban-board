{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "consistent-return": "off",
        "no-underscore-dangle": "off",
        "max-len": ["error", { "code": 120 }]
    }
}
